<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="10dp" >

        <TextView
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_marginLeft="20dp"
            android:text="Click on one of the buttons to \n go to that view"/>

        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="30dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="JSONNN"
            android:id="@+id/testJson" />


        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="30dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sign in view"
            android:id="@+id/testSignIn" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sign up view"
            android:id="@+id/testSignUp" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:text="Helper"/>

        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="... Helper Main GUI ..."
            android:id="@+id/testhelperMainGui" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Helper Search"
            android:id="@+id/testhelperSearch" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Helper Own Profile"
            android:id="@+id/testHelperOwnProfile" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Helper feed"
            android:id="@+id/testHelperFeed" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Helper Search Result"
            android:id="@+id/testHelperSearchResult" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Map Search"
            android:id="@+id/testMapSearch" />



        <TextView
            android:layout_width="300dp"
            android:layout_height="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:text="Needy"/>


        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="needy existing causes "
            android:id="@+id/testNeedExistingCauses" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="...NEEDY MAIN GUI ..."
            android:id="@+id/testNeedyMainGui" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="needy make a cause"
            android:id="@+id/testNeedyMakeACause" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="needy click on own cause"
            android:id="@+id/testNeedyClickOnOwnCause" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="needy profile"
            android:id="@+id/testNeedyProfile" />
        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="needy others click on cause"
            android:id="@+id/testNeedyOthersClickOnCause" />


        <TextView
            android:layout_width="300dp"
            android:layout_height="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:text="Other common"/>

        <Button

            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cause full detail view "
            android:id="@+id/testCauseFullDetailView" />


    </LinearLayout>


</ScrollView>

